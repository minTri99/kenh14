{
  "name": "Kenh14",
  "description": "Kenh 14 get video",
  "version": "1.0",
  "browser_action": {
    "default_icon": "Logo.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://kenh14.vn/*"],
      "js": ["main.js"]
    }
  ],
  "icons": {
    "128": "Logo.png",
    "16": "Logo.png",
    "48": "Logo.png"
},
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "manifest_version": 2
}
